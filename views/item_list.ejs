<!DOCTYPE html>
<html lang="en">
<head>
    <title><%= title %></title>
    <meta lang="en">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" initial-scale="1">
    <link rel="stylesheet" href="/stylesheets/style.css" />
</head>
<body>
    <%- include('layout') -%>
    <h1><%= title %></h1>
    <% if (item_list.length) { %>
        <ul>
            <% item_list.forEach(item => { %>
                <li>
                    <a href="<%= item.url %>"><%= item.title %></a>
                    <p>$<%= item.price %></p>
                </li>
            <% }) %>
            <% } else { %>
                <li>There are no items.</li>
        </ul>
    <% } %>

  
     
</body>
</html>